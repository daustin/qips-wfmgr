<h1>Listing tasks</h1>

<table>
  <tr>
    <th>Process def</th>
    <th>Rank</th>
    <th>Submission</th>
    <th>State</th>
  </tr>

<% @tasks.each do |task| %>
  <tr>
    <td><%=h task.process_def %></td>
    <td><%=h task.rank %></td>
    <td><%=h task.submission_id %></td>
    <td><%=h task.state %></td>
    <td><%= link_to 'Show', task %></td>
    <td><%= link_to 'Edit', edit_task_path(task) %></td>
    <td><%= link_to 'Destroy', task, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New task', new_task_path %>